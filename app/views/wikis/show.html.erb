<h3 class="text-center"><%= @wiki.title %></h3>

<div class="row">
  <div class="col-md-12 text-center">
    <h4>Original Text<h4>
    <p><%= @wiki.body %></p>
  </div>
</div>

<div class="row">
  <div class="col-md-12 text-center">
    <h4>Markdown Text</h4>
    <p><%= markdown(@wiki.body) %></p>
     <%= link_to "Edit Wiki", edit_wiki_path(@wiki), class: 'btn btn-default' %>
   </div>
</div>
