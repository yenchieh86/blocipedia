<div class="container" style="margin-left:55px">
    <div style="margin-left:-1000px;">
        <h3 class="text-center"><%= @wiki.title %></h3>
    </div>
</div>
<div class="container" style="margin-left:-400px">
    <div class="col-md-11">
        <h4 class="text-center"> Original Text <h4><br>
        <div class="col-md-13 text-center" >
            <p class="text-center"><%= @wiki.body %></p>
        </div>
    </div>
</div>
<br><br>
<div class="container" style="margin-left:-400px">
    <div class="col-md-11">
        <h4 class="text-center"> Markdown Text </h4><br>
        <div class="col-md-13 text-center" >
            <p ><%= markdown(@wiki.body) %></p>
        </div>
        
    </div>
</div><br><br>
<div class="container" style="margin-left:80px">
    <button type="button" class="btn btn-default"><%= link_to "Edit Wiki", edit_wiki_path(@wiki) %></button>
</div>

