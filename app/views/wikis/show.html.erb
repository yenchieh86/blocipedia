<div class="col-md-11">
  <h3 class='text-center'>
    <%= @wiki.title %>
  </h3>
  <br/>
  <div class="row">
    <div class="col-md-6">
      <h4 class='text-center'>Original Text</h4>
      <pre><code><%= @wiki.body %></code></pre>
    </div>
    <div class="col-md-6">
      <h4 class='text-center'>Markdown Text</h4>
        <div class="markdowntext text-center">
          <%= markdown(@wiki.body) %>
        </div>
    </div>
  </div>
  <%= link_to "Edit Wiki", edit_wiki_path(@wiki), class: "btn btn-default" %>
  <%= link_to "Collaborator", wiki_collaborators_path(@wiki), class: "btn btn-default" %>
</div>